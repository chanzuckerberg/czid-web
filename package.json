{
  "name": "idseq-web",
  "license": "MIT",
  "repository": "https://github.com/chanzuckerberg/idseq-web",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "npm install --no-optional && webpack --config webpack.config.dev.js --watch",
    "build-img": "webpack --config webpack.config.prod.js",
    "lint": "./node_modules/.bin/eslint app/assets/src --ext .js,.jsx",
    "precommit": "lint-staged",
    "test": "jest",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./enzyme_config.js"
  },
  "lint-staged": {
    "*.{js,jsx,css,scss}": [
      "prettier --write --trailing-comma es5",
      "git add"
    ],
    "*.rb": [
      "bundle exec rubocop --auto-correct --config .rubocop.yml --display-cop-names --display-style-guide",
      "git add"
    ],
    "*.py": [
      "black --line-length 120",
      "flake8 --show-source",
      "git add"
    ]
  },
