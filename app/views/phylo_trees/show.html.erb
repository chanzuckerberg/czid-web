<div id="PhyloTreeView_page">
  <%= javascript_tag do %>
    react_component('PhyloTreeView', {
    csrf: '<%= form_authenticity_token %>',
    admin: '<%= current_user.admin? ? 1 : 0 %>',
    taxon: JSON.parse('<%= raw escape_json(@taxon) %>'),
    project: JSON.parse('<%= raw escape_json(@project) %>'),
    samples: JSON.parse('<%= raw escape_json(@samples) %>'),
    canEdit: <%= @can_edit %>,
    phyloTree: JSON.parse('<%= raw escape_json(@phylo_tree_augmented) %>')
    }, 'PhyloTreeView_page');
  <% end %>
</div>
