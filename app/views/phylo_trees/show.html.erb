<div id="PhyloTree_page">
  <%= javascript_tag do %>
    react_component('PhyloTree', {
    csrf: '<%= form_authenticity_token %>',
    taxon: JSON.parse('<%= raw escape_json(@taxon) %>'),
    project: JSON.parse('<%= raw escape_json(@project) %>'),
    samples: JSON.parse('<%= raw escape_json(@samples) %>'),
    can_edit: <%= @can_edit %>,
    phylo_tree: JSON.parse('<%= raw escape_json(@phylo_tree_augmented) %>')
    }, 'PhyloTree_page');
  <% end %>
</div>
