<p id="notice"><%= notice %></p>

<h1>Pipeline Outputs</h1>

<table>
  <thead>
    <tr>
      <th>Sample</th>
      <th>Project</th>
      <th>Name</th>
      <th>Total reads</th>
      <th>Remaining reads</th>
      <th>Date created</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pipeline_outputs.each do |pipeline_output| %>
      <tr>
        <td><%= link_to pipeline_output.sample.name, pipeline_output.sample %></td>
        <td><%= link_to pipeline_output.sample.project.name, pipeline_output.sample.project if pipeline_output.sample.project %></td>
        <td><%= pipeline_output.name %></td>
        <td><%= pipeline_output.total_reads %></td>
        <td><%= pipeline_output.remaining_reads %></td>
        <td><%= pipeline_output.created_at %></td>
        <td><%= link_to 'Show', pipeline_output %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p>
<%= link_to 'Front portal', root_path %>
</p>
