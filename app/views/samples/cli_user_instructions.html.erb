<div id="cli_user_instructions">
  <%= javascript_tag do %>
    react_component('CliUserInstructions', {
    authToken: '<%= current_user.authentication_token %>',
    email: '<%= current_user.email %>',
    hostGenomes: JSON.parse('<%= raw escape_json(HostGenome.exposed_genomes) %>')
    }, 'cli_user_instructions');
  <% end %>
</div>
