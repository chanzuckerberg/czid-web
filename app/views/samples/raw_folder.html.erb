<div id="RawResultsFolder">
  <%= javascript_tag do %>
    react_component('RawResultsFolder', {
      filePath: JSON.parse('<%= raw escape_json(@file_path) %>'),
      fileList: JSON.parse('<%= raw escape_json(@file_list) %>'),
      sampleName: '<%= @sample.name %>',
      projectName: '<%= @sample.project.name %>'
    }, 'RawResultsFolder');
  <% end %>
</div>
