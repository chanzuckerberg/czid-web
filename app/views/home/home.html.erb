<div id="samples_page">
  <%= javascript_tag do %>
    react_component('Home', {
      pageSize: '<%=@page_size%>',
      csrf: '<%= form_authenticity_token %>',
      user_auth_token: '<%= current_user.authentication_token %>',
      user_email: '<%= current_user.email %>',
      editableProjects: <%= @editable_project_ids %>,
      favorites: JSON.parse('<%= raw escape_json(@favorite_projects) %>'),
      projects: JSON.parse('<%= raw escape_json(@projects)%>')
    }, 'samples_page');
  <% end %>
</div>
