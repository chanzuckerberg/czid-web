<%= javascript_tag do %>
  react_component('DiscoveryViewRouter', {
    domain: "snapshot",
    snapshotShareId: '<%= @share_id %>',
    snapshotProjectName: '<%= @project_name %>',
    snapshotProjectDescription: JSON.parse('<%= raw escape_json(@project_description) %>'),
    projectId: <%= raw("\"#{@project_id}\"") %>,
  }, 'page_content', JSON.parse('<%= raw escape_json(user_context)%>'));
<% end %>
