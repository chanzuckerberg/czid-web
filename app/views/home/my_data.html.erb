<% if current_user.allowed_features.include?("data_discovery") %>

  <%= javascript_tag do %>
    react_component('DiscoveryView', {
      domain: "my_data",
      allowedFeatures: JSON.parse('<%= raw escape_json(current_user.allowed_feature_list) %>'),
    }, 'page_content');
  <% end %>

<% end %>
